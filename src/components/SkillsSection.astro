<section id="skills" class="py-20 px-4">
  <div class="max-w-6xl mx-auto">
    <div class="text-center mb-16 bg-background">
      <h2
        class="skills-title-animate text-4xl md:text-5xl font-bold text-accent mb-6 font-heading"
      >
        {content.skills.title}
      </h2>
      <p
        class="skills-subtitle-animate text-muted-foreground text-lg max-w-2xl mx-auto leading-relaxed font-baseText"
      >
        {content.skills.subtitle}
      </p>
    </div>

    <div class="space-y-20">
      {
        content.skills.categories.map((category, index) => (
          <div class="relative">
            <ContentDivider name={category.name} />
            {/* Skills Grid */}
            <div class="grid md:grid-cols-2 lg:grid-cols-3 gap-6">
              {category.skills.map((skill, skillIndex) => (
                <div
                  class={`group skill-animate skill-delay-${skillIndex % 6}`}
                >
                  <div class="bg-background hover:bg-background/85 rounded-xl border border-accent/20 hover:border-accent/30 p-6  transition-all duration-300 h-full hover:shadow-sm hover:shadow-accent/5">
                    <div class="flex items-center gap-4 mb-4">
                      {/* Icon */}
                      <div class="w-10 h-10 flex items-center justify-center">
                        <svg
                          class="w-8 h-8 text-foreground group-hover:text-accent  duration-300"
                          fill="currentColor"
                          viewBox="0 0 24 24"
                        >
                          <path
                            d={iconPaths[skill.icon] || iconPaths.javascript}
                          />
                        </svg>
                      </div>

                      <div class="flex-1">
                        <h4 class="text-lg font-bold text-foreground font-baseText">
                          {skill.name}
                        </h4>
                        <div class="flex items-center gap-2">
                          <span class="text-sm font-medium text-accent-text font-baseText">
                            {skill.level}
                          </span>
                          <span class="text-xs text-muted-foreground font-baseText">
                            â€¢ {skill.experience}
                          </span>
                        </div>
                      </div>
                    </div>

                    <p class="text-muted-foreground text-sm leading-relaxed font-baseText">
                      {skill.description}
                    </p>
                  </div>
                </div>
              ))}
            </div>
          </div>
        ))
      }
    </div>
  </div>
</section>

<script is:inline>
  document.addEventListener("DOMContentLoaded", () => {
    const observer = new IntersectionObserver(
      (entries) => {
        entries.forEach((entry) => {
          if (entry.isIntersecting) {
            entry.target.classList.add("visible");
          }
        });
      },
      { threshold: 0.1 }
    );

    document
      .querySelectorAll(
        ".skill-animate, .skills-title-animate, .skills-subtitle-animate, .content-divider-animate"
      )
      .forEach((el) => {
        observer.observe(el);
      });
  });
</script>
